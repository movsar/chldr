{
    "collection": "Entry",
    "database": "dosham-database",
    "roles": [
        {
            "name": "anonymousAccessRole",
            "apply_when": {
                "identities": [
                    {
                        "providerType": "anonymous"
                    }
                ]
            },
            "read": true,
            "write": false,
            "insert": false,
            "delete": false,
            "search": true
        },
        {
            "name": "readAll",
            "apply_when": {
                "%%false": {
                    "%function": {
                        "name": "IsAnonymous"
                    }
                }
            },
            "read": true,
            "write": false,
            "insert": false,
            "delete": false,
            "search": true
        }
    ]
}
